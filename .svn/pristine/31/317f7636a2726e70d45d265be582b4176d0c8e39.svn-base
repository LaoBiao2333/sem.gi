.b2 {
    height: 9.02rem;
    overflow: hidden;
    background: url(../images/b2_bg.jpg) no-repeat center;
    background-size: contain;
}
.b2 h2 {
    margin: .49rem 0 .64rem;
    background: url(../images/title_2.png) no-repeat center;
    background-size: contain;
}
.b2 .b2-slider-wrap {
    margin: 0 auto;
    width: 7.16rem;
    overflow: hidden;
    position: relative;
    padding-bottom: 2.67rem;
}
.b2 .b2-slider {
    position: relative;
    width: 9999px;
    left: 0;
    top: 0;
}
.b2 .b2-slider .slider {
    width: 7.16rem;
    height: 4.5rem;
    float: left;
    /* padding: 37px 62px 0; */
}
.b2 .b2-slider .slider img {
    width: 100%;
}
.b2 .b2-slider .slider .slider-box {
    width: 7.08rem;
    height: 4.42rem;
    background-color: #fff;
    border: .04rem solid #1b2b6a;
    border-radius: .3rem;
    text-align: center;
    vertical-align: middle;
    display: table-cell;
    position: relative;
}
.b2 .b2-slider .slider .slider-box img {
    display: inline-block;
    height: 4.17rem;
    width: auto;
    max-width: 1128px;
    /* min-width: 1128px; */
    max-height: 519px;
    /* min-height: 519px; */
}
.b2 .b2-slider .slider span {
    display: inline-block;
    /* visibility: hidden; */
    position: absolute;
    bottom: -1.82rem;
    width: 3.43rem;
}
.b2 .b2-slider .slider.slider1 span.span1 {
    left: 0;
    width: 3.92rem;
    height: 3.05rem;
    background: url(../images/b2_1-1.png) no-repeat center;
    background-size: contain;
}
.b2 .b2-slider .slider.slider1 span.span2 {
    right: 0;
    height: 3.85rem;
    background: url(../images/b2_1-2.png) no-repeat center;
    background-size: contain;
}
.b2 .b2-slider .slider.slider2 span.span1 {
    left: 0;
    height: 3.81rem;
    background: url(../images/b2_2-1.png) no-repeat center;
    background-size: contain;
}
.b2 .b2-slider .slider.slider2 span.span2 {
    right: 0;
    height: 2.77rem;
    background: url(../images/b2_2-2.png) no-repeat center;
    background-size: contain;
}
.b2 .b2-slider .slider span b {
    position: absolute;
    width: .16rem;
    height: .16rem;
    border-radius: .16rem;
    box-sizing: border-box;
    border: .03rem solid #517cc2;
    display: block;
    position: absolute;
    z-index: 3;
    animation: animatepoint 0.7s infinite;
    margin: -10px 0 0 -10px;
}
.b2 .b2-slider .slider.slider1 span.span1 b {
    top: .21rem;
    right: 0;
}
.b2 .b2-slider .slider.slider1 span.span2 b {
    top: 0.2rem;
    right: .78rem;
    animation-delay: 1.7s;
}
.b2 .b2-slider .slider.slider2 span.span1 b {
    top: .21rem;
    right: .75rem;
}
.b2 .b2-slider .slider.slider2 span.span2 b {
    right: 1.23rem;
    top: 0.28rem;
    animation-delay: 1.7s;
}

@-webkit-keyframes fadeInUp {
    from {
      opacity: 0;
      -webkit-transform: translate3d(0, 20%, 0);
      transform: translate3d(0, 20%, 0);
    }
  
    to {
      opacity: 1;
      -webkit-transform: translate3d(0, 0, 0);
      transform: translate3d(0, 0, 0);
    }
  }
  
  @keyframes fadeInUp {
    from {
      opacity: 0;
      -webkit-transform: translate3d(0, 20%, 0);
      transform: translate3d(0, 20%, 0);
    }
  
    to {
      opacity: 1;
      -webkit-transform: translate3d(0, 0, 0);
      transform: translate3d(0, 0, 0);
    }
  }
  
  .fadeInUp {
    -webkit-animation-name: fadeInUp;
    animation-name: fadeInUp;
  }
@-webkit-keyframes fadeInDown {
    from {
      opacity: 0;
      -webkit-transform: translate3d(0, -20%, 0);
      transform: translate3d(0, -20%, 0);
    }
  
    to {
      opacity: 1;
      -webkit-transform: translate3d(0, 0, 0);
      transform: translate3d(0, 0, 0);
    }
  }
  
  @keyframes fadeInDown {
    from {
      opacity: 0;
      -webkit-transform: translate3d(0, -20%, 0);
      transform: translate3d(0, -20%, 0);
    }
  
    to {
      opacity: 1;
      -webkit-transform: translate3d(0, 0, 0);
      transform: translate3d(0, 0, 0);
    }
  }
  
  .fadeInDown {
    -webkit-animation-name: fadeInDown;
    animation-name: fadeInDown;
  }
@keyframes animatepoint {
	0% {
      transform: scale(0.7);
		opacity: 0;
	}
	20% {
      transform: scale(1);
		opacity: 0.8;
	}
	100% {
      transform: scale(2.5);
		opacity: 0;
	}
}
