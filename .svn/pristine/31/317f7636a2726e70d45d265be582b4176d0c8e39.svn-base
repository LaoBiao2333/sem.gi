.banner {
    height: 7.87rem;
    overflow: hidden;
    position: relative;
    background: url(../images/banner.jpg) no-repeat center;
    background-size: cover;
}
.banner .banner-btn {
    width: max-content;
    position: absolute;
    top: 4.93rem;
    left: 50%;
    transform: translateX(-50%);
}
.banner .banner-btn a,
.b3 .b3-btn a {
    width: 2.4rem;
    height: .68rem;
    text-align: center;
    line-height: .68rem;
    border-radius: .04rem;
    font-size: .32rem;
    font-weight: bold;
    display: block;
    color: #fff;
    background:#ef5151;
    float: left;
    transition: all .3s;
}
.banner .banner-btn a.account {
    color: #1b2b6a;
    background:#fff3d6;
    background:-moz-linear-gradient(top, #fff3d6, #ffd496);  
    background:-webkit-gradient(linear,left top, left bottom, from(#fff3d6), to(#ffd496));  
    background:-o-linear-gradient(top, #fff3d6, #ffd496);
    margin-right: 41px; 
}
.banner .banner-btn a:hover {
    width: 240px;
}
