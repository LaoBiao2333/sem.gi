	background-position:-6.8rem -2px ;
}
.cause .item h4{
	font-size: 0.3rem;
	color: #FFFFFF;
	text-align: center;
	margin: 0 0 0.1rem 0;
}
.cause .item p{
	font-size: 0.26rem;
	color: #FFFFFF;
	line-height: 0.36rem;
	text-align: center;
}
.welfare{
	width: 100%;
	padding: 0.8rem 0 ;
}
.welfare h3{
