.banner{
	position: relative;
	width: 100%;
	height: 10.26rem;
	overflow: hidden;
	background: url(../images/banner.jpg) no-repeat center;
	background-size:100% 10.26rem;
}
.banner .tit_1{
	position: absolute;
	left: 50%;
	width: 7.04rem;
	height: 2.51rem;
	margin-left: -3.52rem;
	top: 0.24rem;
}
.banner .ban_btn{
	position: absolute;
	width: 100%;
	top: 2.88rem;
	height: 0.62rem;
	text-align: center;
	font-size: 0;
}
.banner .ban_btn a{
	display: inline-block;
	width: 2.87rem;
	height: 0.62rem;
	border: 2px solid transparent;
	font-size: 0.32rem;
	line-height: 0.62rem;
	box-sizing: border-box;
	border-radius:8px;
	overflow: hidden;
	font-weight: bold;
	margin: 0 0.2rem;
}
.banner .ban_btn .g_btn{
	background: #feed78;
	color: #ce4f2e;
    box-shadow: 2px 2px 8px rgba(176,62,41,.2);
    text-shadow: 1px 1px 1px rgba(255,245,172,.3);
}
.banner .ban_btn .c_btn{
	color: #feed78;
	border: 2px solid #feef7a;
}

.banner .tit_2{
	position: absolute;
	top: 5.18rem;
	left: 50%;
	width: 6.45rem;
	height: 4.94rem;
	margin-left: -3.22rem;
}
.banner .tit_2 .bg{
	position: absolute;
	left: 0;
	top: 0;
}
.banner .tit_2 .mask{
	position: absolute;
	left: 50%;
	bottom: 0.1rem;
	width: 5.06rem;
	height: 2.35rem;
	margin-left: -2.53rem;
	z-index: 10;
}
.banner .tit_2 .money{
	position: absolute;
	left: 0;
	top: 0;
	width: 100%;
	height: 100%;
	z-index: 1;
}
.banner .tit_2 .money1{
	position: absolute;
	left: 50%;
	top:0.8rem;
	width: 4.37rem;
	height: 5.08rem;
    margin-left: -2.3rem;
    z-index: 1;
}
.banner .tit_2 .money2{
	position: absolute;
	left: 0.25rem;
    top: 1.2rem;
	width: 3.19rem;
	height: 3.96rem;

}
.banner .tit_2 .money3{
	position: absolute;
	right: 0.25rem;
    top: 1.2rem;
	width:3.24rem;
	height: 4.58rem;

}
.banner .r_btn{
	position: absolute;
	left: 50%;
	top: 4.1rem;
	margin-left: -0.93rem;
	display: block;
	width: 1.97rem;
	height: 0.74rem;
	background: url(../images/btit2.png) no-repeat;
	background-size:cover ;
	animation: btnf 2s ease-in-out infinite;
}
.banner .carton1{
	position: absolute;
	top: 8.07rem;
	left: 50%;
	width: 1.21rem;
	height: 1.44rem;
    margin-left: -3.05rem;
    z-index: 10;
}

.banner .carton2{
	position: absolute;
	left: 50%;
	width: 1.06rem;
	height: 1.44rem;
	top: 8.08rem;
	margin-left: 1.95rem;
	z-index: 10;
}
.banner .carton2 .eye{
	position: absolute;
	left: 50%;
	top: 0.38rem;
	margin-left: -0.33rem;
	width: 0.66rem;
	height: 0.2rem;
}
.banner .gold .g1{
	position: absolute;
	top: 6.18rem;
	left: 50%;
	margin-left: -3.54rem;
	display: block;
	width: 0.39rem;
	height: 2.24rem;
	z-index: 2;
	animation: adron 3s linear infinite;
	-webkit-animation: adron 3s linear infinite ;
}
.banner .gold .g2{
	position: absolute;
	top: 6.35rem;
	left: 50%;
	margin-left: 2.93rem;
	display: block;
	width: 0.55rem;
	height: 2.08rem;
	z-index: 2;
	animation: adron 3s linear 1s infinite;
	-webkit-animation: adron 3s linear 1s infinite;
}
.flex{
	display: -webkit-flex;
	display: flex;
	align-items: center;
	-webkit-align-items: center;
}

.advantage{
