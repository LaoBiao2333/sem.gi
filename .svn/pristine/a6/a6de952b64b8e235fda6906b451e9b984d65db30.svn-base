    background-size: cover;
}
.b1 ul li.b1-li2 {
    background: url(../images/b1_i2.jpg) no-repeat center;
    background-size: cover;
}
.b1 ul li.b1-li3 {
    background: url(../images/b1_i3.jpg) no-repeat center;
    background-size: cover;
}
.b1 ul li.b1-li4 {
    background: url(../images/b1_i4.jpg) no-repeat center;
    background-size: cover;
}
.b1 ul li.b1-li5 {
    background: url(../images/b1_i5.jpg) no-repeat center;
    background-size: cover;
}
.b1 ul li.b1-li6 {
    background: url(../images/b1_i6.jpg) no-repeat center;
    background-size: cover;
}
.b1 ul li.b1-li7 {
    background: url(../images/b1_i7.jpg) no-repeat center;
    background-size: cover;
}
.b1 ul li.b1-li8 {
    background: url(../images/b1_i8.jpg) no-repeat center;
    background-size: cover;
}
.b1 ul li span {
    width: inherit;
    height: 1.55rem;
    display: table-cell;
    vertical-align: middle;
    /* background-color: rgba(0, 0, 0, .6); */
    text-align: center;
    /* transition: all .3s; */
}
.b1 ul li b {
    font-size: .28rem;
