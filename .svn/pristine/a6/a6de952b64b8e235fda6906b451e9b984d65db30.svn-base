.banner {
    height: 7.86rem;
    overflow: hidden;
    position: relative;
    background: url(../images/banner.jpg) no-repeat center;
    background-size: cover;
}
.banner img {
    width: 7.43rem;
    display: block;
    margin: .63rem auto 0;
}
.banner .btn-box {
    width: 6.2rem;
    text-align: center;
    position: absolute;
    top: 6.42rem;
    left: 50%;
    font-size: 0;
    transform: translateX(-50%);
}
.btn-box a {
    display: inline-block;
    width: 2.8rem;
    height: .8rem;
    text-align: center;
    line-height: .8rem;
    border-radius: .1rem;
    font-size: .3rem;
    background-color: transparent;
    border: 0.02rem solid #f2eabc;
    color: #f2eabc;
    transition: all .3s;
}
.btn-box a.account {
    background-color: #f2eabc;
    color: #b61818;
    margin-right: .5rem;
}

.content-box {
    width: 100%;
    /* height: 2061px; */
    height: 100%;
    background: url(../images/content_bg.jpg) no-repeat center;
    background-size: cover;
    /* background-color: #a71d1b; */
    padding-top: .1rem;
}
.box {
    width: 7.10rem;
    margin: 0 auto 1rem;
    
}

.box h2 {
    margin-bottom: .6rem;
    line-height: 1;
    color: #f2eabd;
    font-size: .36rem;
    text-align: center;
}
.box h2 i {
    width: .31rem;
    height: .23rem;
    display: inline-block;
    background: url(../images/all_icon.png) -.56rem -3.36rem no-repeat;
    background-size: 5.44rem 3.59rem;
    margin-right: .3rem;
}
.box h2 i.i2 {
    margin-right: 0;
    margin-left: .3rem;
    background-position-x: -1.06rem;
}
