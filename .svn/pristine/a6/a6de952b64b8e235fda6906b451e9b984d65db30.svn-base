}


.b2 ul,
.b3 .b3-ul {
    width: 7.1rem;
    height: 4.04rem;
    border-radius: .1rem;
    background: #fff;
    text-align: center;
    display: table-cell;
    vertical-align: middle;
    align-content: center;
    overflow: hidden;
    font-size: 0;
}
.b2 .b2-wrap li {
    width: 2.15rem;
    height: 3.2rem;
    display: inline-block;
}
.b2 .b2-wrap li div {
    width: 2.15rem;
    height: 3.2rem;
    border-radius: .1rem;
    display: inline-block;
    background-color: #e36039;
    border-top: .04rem solid #fca78e;
}
.b2 .b2-wrap li:not(:first-child) {
    margin-left: .16rem;
}
.b2 .b2-wrap li i.b2-icon {
    width: .66rem;
    height: .92rem;
    display: block;
    margin: .43rem auto .36rem;
    background: url(../images/all_icon.png) no-repeat -.49rem 0;
    background-size: 5.44rem 3.59rem;
}
.b2 .b2-wrap li.b2-li2 i.b2-icon {
    width: 1.16rem;
    background-position-x: -1.91rem;
} 
.b2 .b2-wrap li.b2-li3 i.b2-icon {
    width: .9rem;
    background-position-x: -3.76rem;
} 
.b2 .b2-wrap li i.b2-line {
    width: 1.8rem;
    height: .02rem;
    background-color: #ffa387;
    display: block;
    margin: 0 auto;
}
.b2 .b2-wrap li b {
    font-size: .3rem;
    color: #ffe82a;
    line-height: 1;
    display: block;
    margin: .2rem auto .14rem;
}
.b2 .b2-wrap li b i {
    width: .1rem;
    height: .1rem;
    display: inline-block;
    vertical-align: middle;
    background: url(../images/all_icon.png) -1.82rem -3.43rem no-repeat;
    background-size: 5.44rem 3.59rem;
}
.b2 .b2-wrap li b i.i1 {
    margin-right: .15rem;
}
.b2 .b2-wrap li b i.i2 {
    margin-left: .15rem;
}
.b2 .b2-wrap li span {
    font-size: .24rem;
    color: #fff;
    line-height: .36rem;
}
@-webkit-keyframes fadeInUp {
    from {
      opacity: 0;
      -webkit-transform: translate3d(0, 20%, 0);
      transform: translate3d(0, 20%, 0);
    }
  
    to {
      opacity: 1;
      -webkit-transform: translate3d(0, 0, 0);
      transform: translate3d(0, 0, 0);
    }
  }
  
  @keyframes fadeInUp {
    from {
      opacity: 0;
      -webkit-transform: translate3d(0, 20%, 0);
      transform: translate3d(0, 20%, 0);
    }
  
    to {
      opacity: 1;
      -webkit-transform: translate3d(0, 0, 0);
      transform: translate3d(0, 0, 0);
    }
  }
  
  .fadeInUp {
    -webkit-animation-name: fadeInUp;
    animation-name: fadeInUp;
  }
@-webkit-keyframes fadeInDown {
    from {
      opacity: 0;
      -webkit-transform: translate3d(0, -20%, 0);
      transform: translate3d(0, -20%, 0);
    }
  
    to {
      opacity: 1;
      -webkit-transform: translate3d(0, 0, 0);
      transform: translate3d(0, 0, 0);
    }
  }
  
  @keyframes fadeInDown {
    from {
      opacity: 0;
      -webkit-transform: translate3d(0, -20%, 0);
      transform: translate3d(0, -20%, 0);
    }
  
    to {
      opacity: 1;
      -webkit-transform: translate3d(0, 0, 0);
      transform: translate3d(0, 0, 0);
    }
  }
  
  .fadeInDown {
    -webkit-animation-name: fadeInDown;
    animation-name: fadeInDown;
  }


.b3 .b3-ul {
    height: 2.7rem;
    
}
.b3 .b3-ul ul {
    width: max-content;
    display: block;
    margin: 0 auto;
}
.b3 ul li {
    width: 2.3rem;
    height: 1.9rem;
    display: inline-block;
    float: left;
    overflow: hidden;
    color: #fff;
    position: relative;
    padding-left: .24rem;
    box-sizing: border-box;
}
.b3 ul li.b3-li1 {
    z-index: 3;
    background: url(../images/b3_bg1.png) no-repeat center;
    background-size: contain;
}
.b3 ul li.b3-li2 {
    width: 3.52rem;
    margin-left: -1.1rem;
    z-index: 2;
    padding-left: 1.26rem;
    background: url(../images/b3_bg2.png) no-repeat center;
    background-size: contain;
}
.b3 ul li.b3-li3 {
    width: 4.28rem;
    margin-left: -2.36rem;
    padding-left: 2.54rem;
    background: url(../images/b3_bg3.png) no-repeat center;
    background-size: contain;
}
.b3 ul li b.b2 {
    font-size: .3rem;
    display: block;
    text-align: left;
    line-height: 1;
    margin: .24rem 0 .13rem;
}
.b3 ul li span {
    width: 1.5rem;
    font-size: .24rem;
    display: block;
    text-align: left;
    line-height: .36rem;
}

.b4.box {
    margin-bottom: .7rem;
}
.b4 .b4-ul {
    width: 100%;
    /* height: 3.9rem; */
    border-radius: .1rem;
    background: #fff;
    text-align: center;
    overflow: hidden;
    padding: .4rem 0;
    box-sizing: border-box;
}
.b4 .b4-ul .b4-li {
    /* width: 4.1rem; */
    /* height: 2.44rem; */
    height: max-content;
    display: inline-block;
    background-color: #e36039;
    border-top: .04rem solid #fca78e;
    border-radius: .1rem;
    position: relative;
    padding: 1.04rem 0 .3rem;
    box-sizing: border-box;
    opacity: 0;
    margin-top: .68rem;
    /* margin-top: 1.05rem; */
}
.b4 .b4-ul .b4-li.swiper-slide-active {
    opacity: 1;
    display: inline-block;
}
.b4 .b4-ul .b4-li.swiper-slide-prev,
.b4 .b4-ul .b4-li.swiper-slide-next {
    opacity: .8;
}
.b4 .b4-ul .b4-li.swiper-slide-prev i.b4-icon,
.b4 .b4-ul .b4-li.swiper-slide-next i.b4-icon {
    visibility: hidden;
}
.b4 .b4-ul .b4-li i.b4-icon {
    width: 1.6rem;
    height: 1.6rem;
    position: absolute;
    top: -.68rem;
    left: 50%;
    transform: translateX(-50%);
    background: url(../images/all_icon.png) 0 -1.37rem no-repeat;
    background-size: 5.44rem 3.59rem;
    visibility: visible;
}
.b4 .b4-ul .b4-li.b4-li2 i.b4-icon {
    background-position-x: -1.92rem;
}
.b4 .b4-ul .b4-li.b4-li3 i.b4-icon {
    background-position-x: -3.84rem;
}
.b4 .b4-ul .b4-li b {
    font-size: .3rem;
    color: #ffe82a;
    display: block;
    line-height: 1;
}
.b4 .b4-ul .b4-li i.b4-line {
    width: 2.47rem;
    height: .12rem;
    display: block;
    margin: .23rem auto .21rem;
    background: url(../images/all_icon.png) -2.4rem -3.42rem no-repeat;
    background-size: 5.44rem 3.59rem;
}
.b4 .b4-ul .b4-li span {
    font-size: .24rem;
    color: #fff;
    line-height: 1;
}
.b4 .btn-box {
    width: max-content;
    margin: .5rem auto 0;
    display: block;
    /* background-color: #a71d1b; */
}