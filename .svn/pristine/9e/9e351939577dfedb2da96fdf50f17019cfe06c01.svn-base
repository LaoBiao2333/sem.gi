body,
h1,
h2,
h3,
h4,
h5,
h6,
hr,
p,
blockquote,
dl,
dt,
dd,
ul,
ol,
li,
pre,
div,
form,
fieldset,
legend,
button,
input,
textarea,
th,
td,
button,
input,
select,
textarea {
	margin: 0;
	padding: 0;
	line-height: 1;
	font-size: 14px;
	font-family: "PingFang SC", "Microsoft Yahei", "Hiragino Sans GB", arial, sans-serif;
	-webkit-text-size-adjust: 100%;
}

h1,
h2,
h3,
h4,
h5,
h6 {
	font-weight: normal;
}

a {
	text-decoration: none;
}
a,a:hover,a:active,a:visited,a:link,a:focus{
    -webkit-tap-highlight-color:rgba(0,0,0,0);
    -webkit-tap-highlight-color: transparent;
    outline:none;
    text-decoration: none;
}

em {
	font-style: normal;
}

ul,
ol {
	list-style: none;
}

img {
	border: 0;
	vertical-align: middle;
	outline: none;
}

button,
input {
	border: none;
	outline: none;
	-webkit-appearance: none;
	-moz-appearance: none;
	appearance: none;
}

table {
	border-collapse: collapse;
	border-spacing: 0
}

.fl {
	float: left;
}

.fr {
	float: right;
}

.clearfix {
	*zoom: 1;
}

.clearfix:before,
.clearfix:after {
	display: table;
	line-height: 0;
	content: "";
}

.clearfix:after {
	clear: both;
}

.center {
	display: block;
	margin: 0 auto;
}

.content {
	margin-top: 1.6rem;
	width: 100%;
	overflow: hidden;
}

/*******************************************************************
* header
********************************************************************/

.header-fixed {
	width: 100%;
	position: fixed;
	left: 0;
	top: 0;
	z-index: 106; 
	background: #151f2f;
	display: table;
	text-align: center;
	padding: 0 .45rem;
	box-sizing: border-box;
}

.header {
	height: 1.6rem;
	display: table-cell;
    vertical-align: middle;
}

.header .btn {
	display: inline-block;
}

.header .logo {
	width: 2.8rem;
	padding-top: .08rem;
	box-sizing: border-box;
	float: left;
}

.header .logo img {
	display: block;
	width: 100%;
}

.header .menu {
	width: 1.03rem;
	height: .7rem;
	background: url("../images/bar.png") no-repeat center;
	background-size: .46rem .33rem;
	border: #222f44 solid .01rem;
	border-radius: .05rem;
	float: right;
}

/* 菜单menu */
.menu-bg {
	display: none;
	width: 100%;
	height: 100%;
	position: fixed;
	top: 0;
	left: 0;
	z-index: 105;
	background-color: rgba(0,0,0,0);
}

.menu-content {
	/* display: none; */
	width: 4.15rem;
	height: 88%;
	background: #151f2f;
	position: fixed;
	top: 1.6rem;
	right: 0;
	z-index: 119;
	overflow: auto;
	transform: translateX(100%);
	-ms-transform: translateX(100%);
	-moz-transform: translateX(100%);
	-webkit-transform: translateX(100%);
	-o-transform: translateX(100%);
	transition: all .5s;
	transition-timing-function: ease-in-out;
}

.menu-content.open-menu {
	transform: translateX(0);
	-ms-transform: translateX(0);
	-moz-transform: translateX(0);
	-webkit-transform: translateX(0);
	-o-transform: translateX(0);
}

.menu-list {
	width: 100%;
	padding-bottom: 0.98rem;
	/* height: 6.86rem; */
}

.menu-list .item {
	/* height: .98rem; */
	display: block;
}

.menu-list a {
	display: block;
	width: 100%;
	height: .98rem;
	line-height: .98rem;
	text-indent: .4rem;
	font-size: 0.28rem;
	font-weight: bold;
	color: #a7a8ac;
	border-bottom: #303c4e solid 1px;
}

.menu-list a i {
	height: .36rem;
	display: inline-block;
	vertical-align: middle;
	margin-right: .21rem;
	width: .51rem;
	background: url(../images/icon_footer.png) -.52rem 0 no-repeat;
	background-size: 1.49rem .36rem
}

.menu-list .item:last-child .link {
	border-bottom: none;
}

.menu-list li:first-child > a {
	color: #fff;
}

.menu-list .link {
	transition: border-color 0.6s;
}

.menu-list .link .icon {
	display: inline-block;
	width: 0.2rem;
	height: .12rem;
	background: url("../images/menu_icon.png") no-repeat center;
	background-size: cover;
	margin-left: .23rem;
}

.menu-list .link-open {
	border-color: #2a3a48;
}

.menu-list .sub-list {
	display: none;
}

.menu-list .sub-list a {
	background-color: #1b273b;
}

.menu-list .item {
	transition: all 0.5s;
}

/*******************************************************************
* footer
********************************************************************/

.footer{
	margin-bottom: 1.1rem;
	padding: 1.01rem 0.45rem .51rem;
	background: #151f2f;
	box-sizing: border-box;
	overflow: hidden;
}
.footer img{
	display: block;
	width: 2.2rem;
	height: .42rem;
	margin: 0 auto;
}
.footer p{
	font-size: 0.22rem;
	color: #bdbdbd;
	margin-bottom: .64rem;
	line-height: .35rem;
}

.footer-fixed {
	position: fixed;
	left: 0;
	bottom: 0;
	height: 1.1rem;
	width: 100%;
	background: #151f2f;
	z-index: 100;
}
.footer-fixed ul li {
	width: 33.3%;
	height: 100%;
	float: left;
}
.footer-fixed ul li a {
	width: 100%;
	height: 100%;
	color: #fff;
	font-size: .3rem;
	line-height: .8rem;
	margin: .15rem 0;
	text-align: center;
	display: inline-block;
	border-left: #334562 solid 0.01rem;
}
.footer-fixed ul li:first-child a {
	border-left: none;
}
.footer-fixed ul li a i {
	height: .36rem;
	display: inline-block;
	vertical-align: middle;
	margin-right: .21rem;
}
.footer-fixed ul li.footer-li1 a i {
	width: .44rem;
	background: url(../images/icon_footer.png) 0 0 no-repeat;
	background-size: 1.49rem .36rem;
}
.footer-fixed ul li.footer-li2 a i {
	width: .51rem;
	background: url(../images/icon_footer.png) -.52rem 0 no-repeat;
	background-size: 1.49rem .36rem;
}
.footer-fixed ul li.footer-li3 a i {
	width: .38rem;
	background: url(../images/icon_footer.png) -1.11rem 0 no-repeat;
	background-size: 1.49rem .36rem;
}

.footer-fixed ul li.footer-li3 a {
	position: relative;
}

.breathe-btn{ 
	position: absolute;
    top: 0rem;
    right: 0.2rem;
    width: .13rem;
    height: .13rem;
    border-radius: 50%;
	border: 1px solid #ff6200;
    /* border-radius: 5px; */
    color: #fff;
    font-size: 20px;
    text-align: center;
    cursor: pointer;
    box-shadow: 0 1px 2px rgba(0,0,0,.3);
    overflow: hidden;
    background-image: -webkit-gradient(linear, left top, left bottom, from(#f18a4b), to(#ff6200));
    -webkit-animation-timing-function: ease-in-out;
    -webkit-animation-name: breathe;
    -webkit-animation-duration: 2000ms;
    -webkit-animation-iteration-count: infinite;
    -webkit-animation-direction: alternate;
}

@keyframes breathe {
	0% { opacity: .7; box-shadow:0 1px 2px rgba(255,255,255,0.1);}
	100% { opacity: 1; border:1px solid rgb(255, 154, 59); box-shadow:0 0 10px rgb(241, 138, 75);}
}

@-webkit-keyframes breathe {
	0% { opacity: .7; box-shadow:0 1px 2px rgba(255,255,255,0.1);}
	100% { opacity: 1; border:1px solid rgb(255, 154, 59); box-shadow:0 0 10px rgb(241, 138, 75);}
}

.wave {  
	position: absolute;  
	top: -.05rem;
    right: 0.01rem;
	width: .4rem;  
	height: .4rem;
	display: flex;
    align-items: center;
    justify-content: center;
}  

.wave .circle {  
	width: 100%;
	height: 100%;
	position: absolute;
	top: 0;
	left: 0;  
	border-radius: 50%;  
	opacity: 0;
	background: #ff6200;  
	z-index: -1;
	position: absolute;
	left: 50%;
	top: 50%;
	margin-left: -.2rem;
	margin-top: -.2rem;
	/* transform: translate(-50%,-50%); */
}

.wave .circle:first-child {  
	animation: circle-opacity 2s linear infinite; 
	-webkit-animation: circle-opacity 2s linear infinite; 
}  

.wave .circle:nth-child(2) { 
	opacity: 1;
	transform: scale(.35); 
	/* animation: circle-opacity 6s linear infinite;  
	-webkit-animation: circle-opacity 6s linear infinite; 
	animation-delay: 3s;   */
}  

@keyframes circle-opacity{  
	0% {  
		opacity: 0.5;  
		transform: scale(.4);  
	}  
	50% {  
		opacity: .3;  
		transform: scale(.8);  
	} 
	100% {  
		opacity: 0.5;  
		transform: scale(.4);  
	} 
}  

 body{
	position: relative;
}
.mask{
	position: absolute;
	display: none;
	width: 100%;
	height: 100%;
	background: #161f27;
	z-index: 19;
}
@media only screen and (min-width:1025px ) {
	body{
		width: 750px;
		margin: 0 auto;
	}
	.header-fixed, .study-cnt .reservation {
	    width: 750px;
	    left: 50%;
	    margin-left: -375px;
    }

} 

.footer p .skynet-i {
	width: .2rem;
	height: .22rem;
	background: url(../images/skynet_btn.png) no-repeat center;
	background-size: contain;
	display: inline-block;
}

/* 天网系统样式 */
.skynet-box {
	visibility: hidden;
	position: fixed;
	top: 0;
	left: 0;
	width: 100%;
	height: 100%;
	z-index: 999;
}

.skynet-cover {
	width: 100%;
	height: 100%;
	background-color: #000;
	opacity: 0.5;
	position: absolute;
}

.skynet-box .skynet-content  {
	width: 6.7rem;
	position: absolute;
	top: 50%;
    left: 50%;
    transform: translate(-50%,-50%);
	height: 4.2rem;
	border-radius: .2rem;
	overflow: hidden;
	transition: all .5s;
}

.skynet-box .skynet-content > div {
	background-color: #fff;
	float: left;
	z-index: 999;
	height: 4.2rem;
	text-align: center;
}

.skynet-box .skynet-popup {
	width: 6.7rem;
	height: 100%;
	position: absolute;
}

.skynet-box div .popup-top {
	height: .71rem;
	background-color: #151f2f;
	position: relative;
	font-size: .3rem;
	color: #fff;
	width: 6.7rem;
    display: flex;
    align-items: center;
}

.skynet-box .skynet-popup .popup-top a {
	width: 1.73rem;
    height: .33rem;
	display: inline-block;
	background: url(../images/skynet_logo.png) no-repeat center;
	background-size: contain;
	margin-left: .5rem;
}

.skynet-box .skynet-popup .popup-top i {
	width: .28rem;
	height: .29rem;
	display: inline-block;
	background: url(../images/close_skynet.png) no-repeat center;
	background-size: contain;
	position: absolute;
	right: .25rem;
	top: .25rem;
	cursor: pointer;
}

.skynet-box .skynet-popup .popup-middle {
	width: 100%;
	margin: .78rem auto .7rem;
	padding: 0 .34rem;
	box-sizing: border-box;
	position: relative;
	display: flex;
	align-items: center;
	justify-content: center;
}

.skynet-box .skynet-popup .popup-tip {
	color: #666;
	font-size: .26rem;
    position: absolute;
    bottom: -.35rem;
	left: 50%;
	margin-left: -3.1rem;
	padding-left: .14rem;
}

.skynet-box .skynet-popup .popup-middle .input-div {
	width: 6rem;
	height: .72rem;
	line-height: .72rem;
	border: #aaa solid 1px;
	font-size: .28rem;
	color: #999;
	vertical-align: middle;
	display: flex;
	align-items: center;
}

.skynet-box .skynet-popup .popup-middle input {
	width: 6rem;
	height: .28rem;
	line-height: normal;
	border: none;
	font-size: .28rem;
	color: #999;
	box-sizing: border-box;
	border: 1px solid transparent;
	vertical-align: middle;
	padding: .2rem .14rem;
	display: flex;
	align-items: center;
	background-color: transparent;
}


.skynet-box > div .popup-bottom a {
	width: 2.4rem;
    height: .64rem;
    line-height: .64rem;
    border-radius: .32rem;
    color: #fff;
    font-size: .28rem;
	background-color: #484554;
	text-align: center;
	display: inline-block;
}

.skynet-box .skynet-popup .popup-bottom a.last-step.active {
	background-color: #ff6200;
}

.skynet-box div.skynet-popup .popup-bottom a:not(.last-step) {
	display: none;
}

.skynet-box .skynet-popup .popup-bottom a.copy-btn {
	background-color: #ff6200;
	margin-left: .4rem;
}