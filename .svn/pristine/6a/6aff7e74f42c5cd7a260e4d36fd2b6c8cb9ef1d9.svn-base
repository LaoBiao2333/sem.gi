/*右侧*/
.fixed_right{
	position: fixed;
	right: 10px;
	top: 50%;
	margin-top: -179px;
	width: 188px;
	height: 358px;
	background: url(../images/fixed_r.png) no-repeat;
	z-index: 10;
}
.fixed_right i{
	position: absolute;
	display: block;
	left: 50%;
	bottom: 55px;
	margin-left: -72px;
	width: 145px;
	height: 102px;
	background: url(../images/fixed_z.jpg) no-repeat;
	cursor: pointer;
}
.fixed_right .on{
	transform-origin: top center; 
	animation: shake 1s linear;
}
@keyframes fixed_r{
	0%{
		transform: rotate(0);
	}
	33%{
		transform: rotate(5deg);
	}
	60%{
		transform: rotate(-5deg);
	}
	100%{
		transform: rotate(0deg);
	}
}
.fixed_right a{
	position: absolute;
	bottom: 14px;
	left: 50%;
	margin-left: -68px;
	display: block;
	width: 137px;
	height: 28px;
	line-height: 28px;
	font-size: 16px;
	color: #7d5808;
	background: #fff1c8;
	transition: all .3s;
	text-align: center;
	border-radius: 15px;
}
.fixed_right a:hover{
	background: #f8d369;
}
/*底部固定*/
body{
	padding-bottom: 85px;
}
.fixed_bottom{
	position: fixed;
	display: none;
	bottom: 0;
	left: 0;
	width: 100%;
	height: 85px;
	background: url(../images/fixed_bottom.jpg) no-repeat center;
	z-index: 10;
}
.fixed_bottom .container{
	position: relative;
}
.fixed_bottom i{
	position: absolute;
	display: block;
	right: 12px;
	top: 13px;
	width: 21px;
	height: 21px;
	background: url(../images/close_fixed.png) no-repeat;	
	cursor: pointer;
}
.fixed_bottom .btn_box{
	position: relative;
	float: right;
	font-size: 0;
	width: 430px;
	height: 44px;
	margin: 21px 75px 0 0;
	text-align: center;
}
.fixed_bottom .btn_box a{
	display: block;
	position: absolute;
	top: 0;
	width: 164px;	
	height: 42px;
	font-size: 22px;
	line-height: 42px;
	text-align: center;
	border-radius: 5px;	
	border: 1px solid transparent;
	transition: all .3s;
}
.fixed_bottom .btn_box .g_btn{
	color:#b71b1c ;
	left: 24px;
	background: -webkit-linear-gradient(top, #deba92 30%, #fff6cb);
    background: -o-linear-gradient(top, #deba92 30%, #fff6cb);
    background: -moz-linear-gradient(top, #deba92 30%, #fff6cb);
    background: linear-gradient(to top, #deba92 30%, #fff6cb);
    background: #deba92\0;
}
.fixed_bottom .btn_box .r_btn{
	color: #fff6cb;
	right: 24px;
	border: 1px solid #DEBA92;
	background: -webkit-linear-gradient(top, #b71b1b , #e92e2e);
    background: -o-linear-gradient(top, #b71b1b , #e92e2e);
    background: -moz-linear-gradient(top, #b71b1b , #e92e2e);
    background: linear-gradient(to top, #b71b1b , #e92e2e);
    background: #e92e2e\0;;
}
.fixed_bottom .btn_box .g_btn:hover{
	width: 180px;
	left: 16px;	
}
.fixed_bottom .btn_box .r_btn:hover{
	width: 180px;
	right: 16px;	
}
/*下载弹窗*/
.dowanload_madal{
	position: fixed;
	display: none;
	left: 0;
	top: 0;
	width: 100%;
	height: 100%;
	background: rgba(0,0,0,0.25);
	background: url(../images/banner_bg.png) repeat\0;
	z-index: 100;
}
.dowanload_madal.show{
	display: block;
}
.dowanload_madal .modal{
	position: absolute;	
	left: 50%;
	top: 50%;
	margin-left: -510px;
	margin-top: -300px;
	width: 1020px;
	height: 600px;
	background: url(../images/dl_bg.png) no-repeat center\0;
	opacity: 0;
	transition: all 0.3s;
}

@-webkit-keyframes show_modal {
  from {
    opacity: 0;
    -webkit-transform: translate3d(0, -10%, 0);
    transform: translate3d(0, -10%, 0);
  }

  to {
    opacity: 1;
    -webkit-transform: translate3d(0, 0, 0);
    transform: translate3d(0, 0, 0);
  }
}

@keyframes show_modal {
  from {
    opacity: 0;
    -webkit-transform: translate3d(0, -10%, 0);
    transform: translate3d(0, -10%, 0);
  }

  to {
    opacity: 1;
    -webkit-transform: translate3d(0, 0, 0);
    transform: translate3d(0, 0, 0);
  }
}

.dowanload_madal .show_modal {
  -webkit-animation-name: show_modal;
  animation-name: show_modal;
}
@-webkit-keyframes hide_modal {
  from {
    opacity: 1;
    -webkit-transform: translate3d(0, 0, 0);
    transform: translate3d(0, 0, 0);
  }

  to {
    opacity: 0;
    -webkit-transform: translate3d(0,10%, 0);
    transform: translate3d(0, 10%, 0);
  }
}

@keyframes hide_modal {
  from {
    opacity: 1;
    -webkit-transform: translate3d(0, 0, 0);
    transform: translate3d(0, 0, 0);
  }

  to {
    opacity: 0;
    -webkit-transform: translate3d(0, 10%, 0);
    transform: translate3d(0, 10%, 0);
  }
}

.dowanload_madal .hide_modal {
  -webkit-animation-name: hide_modal;
  animation-name: hide_modal;
}

