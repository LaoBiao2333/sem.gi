.banner {
    height: 850px;
    overflow: hidden;
    position: relative;
    background: url(../images/banner.jpg) no-repeat center;
}
.banner .banner-move {
    width: 100%;
    position: absolute;
    top: 507px;
    left: 0;
}
.banner img {
    width: 1020px;
    display: block;
    margin: 89px auto 0;
}
.banner .btn-box {
    width: max-content;
    text-align: center;
    position: absolute;
    top: 571px;
    left: 50%;
    transform: translateX(-50%);
}
.btn-box a {
    display: inline-block;
    width: 240px;
    height: 60px;
    text-align: center;
    line-height: 60px;
    border-radius: 10px;
    font-size: 30px;
    background-color: #b61818;
    color: #f2eabc;
    transition: all .3s;
}
.btn-box a.account {
    background-color: #f2eabc;
    color: #b61818;
    margin-right: 40px;
}
.btn-box a:hover {
    padding: 0 10px;
}

.content-box {
    width: 100%;
    /* height: 2061px; */
    height: 100%;
    background: url(../images/content_bg.jpg) no-repeat center;
    background-color: #a71d1b;
    padding-top: 10px;
}
.box {
    width: 1200px;
    margin: 0 auto 80px;
    
}

.box h2 {
    margin-bottom: 50px;
    line-height: 1;
    color: #f2eabd;
