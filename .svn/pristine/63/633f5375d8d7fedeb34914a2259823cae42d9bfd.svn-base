}
.box h2 i {
    width: 31px;
    height: 24px;
    display: inline-block;
    background: url(../images/all_icon.png) -19px -278px no-repeat;
    margin-right: 30px;
}
.box h2 i.i2 {
    margin-right: 0;
    margin-left: 30px;
    background-position-x: -87px;
}
